<?xml version="1.0"?>
<launch>
    <!-- 
        机械臂关节基础控制启动文件
        同时启动发布节点和订阅节点
        赛事适配：5轴机械臂，话题/arm_joint_angles
    -->

    <!-- 发布节点：arm_joint_publisher -->
    <node 
        name="arm_joint_publisher" 
        pkg="arm_control" 
        type="arm_joint_publisher.py" 
        output="screen"
        required="true"   
    />

    <!-- 订阅节点：arm_state_subscriber -->
    <node 
        name="arm_state_subscriber" 
        pkg="arm_control" 
        type="arm_state_subscriber.py" 
        output="screen"
    />

</launch>